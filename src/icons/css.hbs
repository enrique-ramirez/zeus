@font-face {
  font-family: "{{fontName}}";
  font-display: swap;
  src: {{{src}}};
}

{{baseSelector}} {
  line-height: 1;
}

{{baseSelector}}:before {
  font-family: {{fontName}} !important;
  font-style: normal;
  font-weight: normal !important;
  vertical-align: middle;
}

{{#each codepoints}}
{{../baseSelector}}.{{../classPrefix}}{{@key}}:before {
  content: "\\{{this}}";
}
{{/each}}
